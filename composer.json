{
    "name": "blank-framework/invokable-controller-route",
    "description": "Library that contains components for creating routes using invokable controller classes",
    "type": "library",
    "require": {
        "php": "^8.0",
        "blank-framework/routing-interfaces": "^1.0",
        "psr/http-message": "^2.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "squizlabs/php_codesniffer": "^3.10",
        "pestphp/pest": "^3.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BlankFramework\\InvokableControllerRoute\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Adrian du Plessis",
            "email": "adrian@blankframework.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
